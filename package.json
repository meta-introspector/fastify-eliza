{
  "type": "module",
  "name": "eliza-fastify",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@autotelic/fastify-opentelemetry": "^0.22.0",
    "@fastify/autoload": "^6.0.0",
    "@fastify/sensible": "^6.0.0",
    "@opentelemetry/exporter-zipkin": "^1.30.0",
    "@opentelemetry/instrumentation": "^0.57.0",
    "@opentelemetry/instrumentation-graphql": "^0.47.0",
    "@opentelemetry/instrumentation-http": "^0.57.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-trace-base": "^1.30.0",
    "@opentelemetry/sdk-trace-node": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "api\"": "link:\"@opentelemetry/api\"",
    "core\"": "link:\"@opentelemetry/core\"",
    "env-schema": "^6.0.1",
    "exporter-zipkin\"": "link:\"@opentelemetry/exporter-zipkin\"",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.3.0",
    "fastify-opentelemetry\"": "link:\"@autotelic/fastify-opentelemetry\"",
    "fastify-plugin": "^5.0.0",
    "fluent-json-schema": "^5.0.0",
    "instrumentation\"": "link:\"@opentelemetry/instrumentation\"",
    "instrumentation-graphql\"": "link:\"@opentelemetry/instrumentation-graphql\"",
    "instrumentation-http\"": "link:\"@opentelemetry/instrumentation-http\"",
    "resources\"": "link:\"@opentelemetry/resources\"",
    "sdk-trace-base\"": "link:\"@opentelemetry/sdk-trace-base\"",
    "sdk-trace-node\"": "link:\"@opentelemetry/sdk-trace-node\"",
    "semantic-conventions\"": "link:\"@opentelemetry/semantic-conventions\"",
    "session\"": "link:\"@fastify/session\""
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-print-routes": "^4.0.1",
    "fastify-tsconfig": "^2.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.6.3"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --address 0.0.0.0 --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
